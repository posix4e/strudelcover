{
  "name": "@strudel/strudelcover",
  "version": "0.1.0",
  "description": "AI-powered music pattern generator using Strudel.cc",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "strudelcover": "./src/cli.js"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "node --test test/unit/**/*.test.js",
    "test:integration": "node --test test/integration/**/*.test.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "node test/smoke.test.js",
    "cover": "node src/cli.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "chalk": "^5.0.0",
    "commander": "^11.0.0",
    "dotenv": "^16.0.0",
    "node-fetch": "^3.3.0",
    "open": "^9.0.0",
    "ora": "^7.0.0",
    "playwright": "^1.53.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "@playwright/test": "^1.48.0"
  },
  "keywords": [
    "strudel",
    "audio",
    "music",
    "cover",
    "ai",
    "generation"
  ],
  "author": "",
  "license": "AGPL-3.0+"
}